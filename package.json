{
  "name": "react-app",
  "version": "1.0.0",
  "description": "AplicaciÃ³n React con sitio web de ejemplo y tests E2E automatizados",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.54.1",
    "@types/node": "^24.0.13",
    "http-server": "^14.1.1",
    "wait-on": "^8.0.4"
  },
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "playwright:test": "playwright test",
    "playwright:test:headed": "playwright test --headed",
    "playwright:test:debug": "playwright test --debug",
    "playwright:test:ui": "playwright test --ui",
    "playwright:test:report": "playwright test && playwright show-report",
    "playwright:show-report": "playwright show-report",
    "playwright:report:open": "playwright show-report --host 0.0.0.0 --port 9323",
    "playwright:report:generate": "playwright test --reporter=html",
    "playwright:report:serve": "npx http-server playwright-report -p 8080 -o",
    "ci:build": "npm ci && npm run build",
    "ci:test": "npm start & npx wait-on http://localhost:3000 && npx playwright test",
    "ci:local": "npm run ci:build && npm run ci:test"
  },
  "keywords": [
    "react",
    "travel",
    "website",
    "playwright",
    "e2e",
    "testing",
    "automation"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
